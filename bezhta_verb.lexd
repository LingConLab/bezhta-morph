VerbRoot(1)[s,c,-cl] Antipassive[s]? VerbNumber[s]? VerbRoot(2)[s,-cl,c] [<v>:] Present[s,c]
VerbRoot(1)[s,l,-cl] Antipassive[s]? VerbNumber[s]? VerbRoot(2)[s,-cl,l] [<v>:] Present[s,l]
# gender-agreeing with umlaut
Class | ClassPl VerbRoot(1)[s,v,cl] Antipassive[s]? VerbRoot(2)[s,cl,v] [<v>:] Present[s,v]
Class VerbRoot(1)[s,v,cl] Antipassive[s]? VerbNumber VerbRoot(2)[s,cl,v] [<v>:] Present[s,v]
Class | ClassPl VerbRoot(1)[s,c,cl] Antipassive[s]? VerbRoot(2)[s,cl,c] [<v>:] Present[s,c]
ClassPl VerbRoot(1)[s,c,cl] Antipassive[s]? VerbNumber VerbRoot(2)[s,cl,c] [<v>:] Present[s,c]
