VerbRootInf(1)[h,а] ClassInfix[h] Antipassive[h]? VerbNumber[h]? VerbRootInf(2)[h,а] [<v>:]  Mood[imp,h,а](Mood[prmI,h] > [<prmII>:ъ])?
VerbRootInf(1)[h,0] ClassInfix[h] Antipassive[h]? VerbNumber[h]? VerbRootInf(2)[h,0] [<v>:]  Mood[imp,h,0](Mood[prmI,h] > [<prmII>:ъ])?

