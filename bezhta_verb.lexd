ClassPl VerbRoot(1)[s,l,cl] Antipassive[s]? VerbNumber VerbRoot(2)[s,cl,l] [<v>:] Present[s,l]

VerbRootInf(1)[s,v] ClassInfix[s] Antipassive[s]? VerbNumber[s]? VerbRootInf(2)[s,v] [<v>:] Present[s,v]
VerbRootInf(1)[s,c] ClassInfix[s] Antipassive[s]? VerbNumber[s]? VerbRootInf(2)[s,c] [<v>:] Present[s,c]
VerbRootInf(1)[s,l] ClassInfix[s] Antipassive[s]? VerbNumber[s]? VerbRootInf(2)[s,l] [<v>:] Present[s,l]

